        {
          "name": "login",
          "description": "Login to the API",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/login",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
